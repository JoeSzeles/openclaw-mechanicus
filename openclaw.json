{
  "gateway": {
    "bind": "loopback",
    "port": 5001,
    "trustedProxies": ["127.0.0.0/8", "172.16.0.0/12", "10.0.0.0/8"],
    "controlUi": {
      "allowInsecureAuth": true,
      "allowedOrigins": ["https://be1a940b-924a-449d-ae02-954c66974b04-00-39aqxi9w7wyw6.picard.replit.dev", "http://127.0.0.1:5000", "http://localhost:5000"]
    }
  },
  "models": {
    "mode": "merge",
    "providers": {
      "openai": {
        "baseUrl": "${AI_INTEGRATIONS_OPENAI_BASE_URL}",
        "apiKey": "${AI_INTEGRATIONS_OPENAI_API_KEY}",
        "api": "openai-completions",
        "models": [
          {
            "id": "gpt-4o",
            "name": "GPT-4o",
            "reasoning": false,
            "input": ["text", "image"],
            "contextWindow": 128000,
            "maxTokens": 16384
          },
          {
            "id": "gpt-4o-mini",
            "name": "GPT-4o Mini",
            "reasoning": false,
            "input": ["text", "image"],
            "contextWindow": 128000,
            "maxTokens": 16384
          }
        ]
      }
    }
  },
  "agents": {
    "defaults": {
      "model": {
        "primary": "openai/gpt-4o"
      },
      "models": {
        "openai/gpt-4o": {
          "alias": "GPT-4o"
        },
        "openai/gpt-4o-mini": {
          "alias": "GPT-4o Mini"
        }
      }
    }
  },
  "auth": {
    "profiles": {
      "openai:default": {
        "provider": "openai",
        "mode": "api_key"
      }
    }
  }
}
